" | $at_diff - "$at_stdout" || at_failed=:
case $at_status in
   77) echo 77 > "$at_status_file"; exit 77;;
   0) ;;
   *) echo "$at_srcdir/append01.at:34: exit code was $at_status, expected 0"
      at_failed=:;;
esac
