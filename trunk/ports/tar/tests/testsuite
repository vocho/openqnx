cd ..
chmod -w dir

echo Create the archive
tar cf archive dir || exit 1

echo Extract
mkdir out
tar -C out -xvf archive
) ) >"$at_stdout" 2>"$at_stderr"
    at_status=$?
fi

at_failed=false
$at_diff "$at_devnull" "$at_stderr" || at_failed=:
echo >>"$at_stdout"; echo "Prepare the directory
